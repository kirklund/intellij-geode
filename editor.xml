<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Source Code Pro" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern case-sensitive="true" pattern="\bTODO\:KIRK\b.*" />
    <pattern case-sensitive="true" pattern="\bTODO\:GEODE-4769\b.*" />
    <filter name="TODO:KIRK">
      <pattern index="2" />
    </filter>
    <filter name="TODO:GEODE-4769">
      <pattern index="3" />
    </filter>
  </component>
</application>